if (!require("pacman")) install.packages("pacman"); library(pacman)
p_load(car) # En este paquete, vive la base "Prestige" ("Prestigio").
# Mira como se ve la base de datos
head(Prestige)
# Extrae la 'x' para conseguir beta1
x = Prestige$education
## Pero recuerda que para conseguir beta0 debes anexar una columna de 1's.
unos = rep(1,length(x))
# Aqui le decimos a R que repita 1s, del largo ("lenght") de 'x'.
# Ahora junta ambas cosas
x = matrix(
c(unos, x), # meter vectores 'unos' y 'x'
ncol = 2 # y organizarlos en 2 columnas.
)
# Extrae la 'y'
y = Prestige$prestige
lm(y ~ x)
lm(Prestige$prestige ~ Prestige$education)
plot(Prestige$prestige ~ Prestige$education)
